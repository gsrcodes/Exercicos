#include <stdio.h>
#define TF 10

int main(void)
{
    int vet[TF], TL = 0, TL2, aux, i, num;
    scanf("%d", &num);

    while (num > 0 && TL < TF) // Atribui os valores ao vetor (0 para parar, ou tamanho lógico igual ao tamanho físico)
    {
        vet[TL] = num;
        scanf("%d", &num);
        TL++;
    }

    for (TL2 = TL, i = 0; i < TL2; i++) // Começando da posição 0, faz os próximos comandos executarem em todas as posições
        for (TL = 0; TL < TL2 -1; TL++)
            if (vet[TL] < vet[TL + 1]) // Compara com a seguinte posição, se menor então substitui
            {
                aux = vet[TL];
                vet[TL] = vet[TL + 1];
                vet[TL + 1] = aux;
            }
    for (TL = 0; TL < TL2; TL++) // Printa o vetor já em ordem decrescente
        printf("%d\n", vet[TL]);
}
